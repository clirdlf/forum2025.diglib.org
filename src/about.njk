<section id="team" class="py-10 px-8">
    <div class="max-w-[90rem] mx-auto grid grid-cols-full items-start gap-y-12 xl:grid-cols-12">
        <div class="col-span-4 xl:max-w-sm">
            <h2 class="block antialiased tracking-normal font-sans text-base font-semibold leading-relaxed text-blue-gray-900 lg:text-lg text-base">
                Meet the Team
            </h2>
            <h3 class="block antialiased tracking-normal font-sans text-4xl font-semibold leading-[1.3] text-blue-gray-900 my-2 text-2xl leading-snug lg:text-4xl">
                The Heart of Our Excellence
            </h3>
            <p class="block antialiased font-sans text-xl font-normal leading-relaxed font-normal text-gray-600">
                DLF Staff and volunteers work year round to make DLF Forum a special event. Here are the people that make this happen!  
            </p>
        </div>

        <div class="xl:col-span-8 col-span-12 grid gap-4 grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-3 xl:ml-auto">
            {% for member in team %}
                <div class="relative flex flex-col bg-clip-border rounded-xl bg-transparent text-gray-700 shadow-none w-full h-full">
                    <div class="relative bg-clip-border !mt-4 mx-4 rounded-xl overflow-hidden bg-transparent text-gray-700 shadow-none mx-0 mt-0 p-0 h-[360px] w-full rounded-2xl">
                        <img eleventy:widths="200,600" src="/{{ member.image }}" alt="{{ member.name }}" class="relative h-full w-full object-cover object-bottom"/>
                        <div class="to-bg-deep-purple-90 absolute left-0 bottom-0 h-1/2 w-full bg-gradient-to-t from-black/50 via-black/50 to-transparent"></div>
                    </div>
                    <div class="p-6 absolute z-10 w-full h-full grid justify-center items-end" >
                        <div>
                            <h3 class="block antialiased tracking-normal font-sans text-xl font-semibold leading-snug text-white text-center text-lg" >
                                {{ member.name }}
                            </h3>
                            <p class="block antialiased font-sans text-sm font-light leading-normal text-center font-normal uppercase text-white">
                                {{ member.role}}
                            </p>
                            <div class="flex items-center justify-center gap-1 mt-2"></div>
                        </div>
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>
</section>

